// The command-line parameters
struct Param {
  // multiple modules can be addressed in parallel, killed, etc. using "module_id"
  string module_id;
};

// This is the WriteModule interface, just outputs values
interface WriteModule {

	// Output is one value each time
	void Output(out long output);

};


